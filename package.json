    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-openapi": "^12.0.1",
